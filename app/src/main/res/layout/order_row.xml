<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/mainLayout">

    <androidx.cardview.widget.CardView
        app:cardBackgroundColor="@color/material_on_surface_stroke"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints" >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="12dp"
            >

            <TextView
                android:id="@+id/textView41"
                android:layout_width="100dp"
                android:layout_height="18dp"
                android:text="E-Mail :"
                app:layout_constraintStart_toStartOf="@+id/textView33"
                app:layout_constraintTop_toBottomOf="@+id/textView33" />

            <TextView
                android:id="@+id/qty_txt"
                android:layout_width="140dp"
                android:layout_height="18dp"
                android:text="TextView"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@+id/phoneNum_txt"
                app:layout_constraintTop_toBottomOf="@+id/phoneNum_txt" />

            <TextView
                android:id="@+id/textView39"
                android:layout_width="100dp"
                android:layout_height="18dp"
                android:text="Quantity :"
                app:layout_constraintStart_toStartOf="@+id/textView37"
                app:layout_constraintTop_toBottomOf="@+id/textView37" />

            <TextView
                android:id="@+id/phoneNum_txt"
                android:layout_width="140dp"
                android:layout_height="18dp"
                android:text="TextView"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@+id/email_txt"
                app:layout_constraintTop_toBottomOf="@+id/email_txt" />

            <TextView
                android:id="@+id/textView37"
                android:layout_width="100dp"
                android:layout_height="18dp"
                android:text="Phone Number :"
                app:layout_constraintStart_toStartOf="@+id/textView41"
                app:layout_constraintTop_toBottomOf="@+id/textView41" />

            <TextView
                android:id="@+id/email_txt"
                android:layout_width="140dp"
                android:layout_height="18dp"
                android:text="TextView"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@+id/city_txt"
                app:layout_constraintTop_toBottomOf="@+id/city_txt" />

            <TextView
                android:id="@+id/city_txt"
                android:layout_width="140dp"
                android:layout_height="18dp"
                android:text="TextView"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@+id/streetAddress_txt"
                app:layout_constraintTop_toBottomOf="@+id/streetAddress_txt" />

            <TextView
                android:id="@+id/textView33"
                android:layout_width="100dp"
                android:layout_height="18dp"
                android:text="City :"
                app:layout_constraintStart_toStartOf="@+id/textView31"
                app:layout_constraintTop_toBottomOf="@+id/textView31" />

            <TextView
                android:id="@+id/streetAddress_txt"
                android:layout_width="140dp"
                android:layout_height="18dp"
                android:text="TextView"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@+id/lastName_txt"
                app:layout_constraintTop_toBottomOf="@+id/lastName_txt" />

            <TextView
                android:id="@+id/lastName_txt"
                android:layout_width="140dp"
                android:layout_height="18dp"
                android:text="TextView"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@+id/firstName_txt"
                app:layout_constraintTop_toBottomOf="@+id/firstName_txt" />

            <TextView
                android:id="@+id/firstName_txt"
                android:layout_width="140dp"
                android:layout_height="18dp"
                android:text="TextView"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@+id/nic_txt"
                app:layout_constraintTop_toBottomOf="@+id/nic_txt" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="100dp"
                android:layout_height="18dp"
                android:text="Item Name :"
                app:layout_constraintStart_toStartOf="@+id/textView13"
                app:layout_constraintTop_toBottomOf="@+id/textView13" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="100dp"
                android:layout_height="18dp"
                android:layout_marginStart="32dp"
                android:layout_marginLeft="32dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="24dp"
                android:text="Item Id :"
                app:layout_constraintBottom_toBottomOf="@+id/nic_txt"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/nic_txt"
                app:layout_constraintVertical_bias="0.153" />

            <TextView
                android:id="@+id/id_txt"
                android:layout_width="27dp"
                android:layout_height="44dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginEnd="24dp"
                android:layout_marginRight="24dp"
                android:text="1"
                android:textSize="30sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toStartOf="@+id/textView15"
                app:layout_constraintHorizontal_bias="0.179"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/nic_txt"
                android:layout_width="140dp"
                android:layout_height="18dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:text="TextView"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@+id/textView13"
                app:layout_constraintTop_toTopOf="@+id/textView13" />

            <TextView
                android:id="@+id/textView30"
                android:layout_width="100dp"
                android:layout_height="18dp"
                android:text="Last_Name :"
                app:layout_constraintStart_toStartOf="@+id/textView15"
                app:layout_constraintTop_toBottomOf="@+id/textView15" />

            <TextView
                android:id="@+id/textView31"
                android:layout_width="100dp"
                android:layout_height="18dp"
                android:text="Street Address"
                app:layout_constraintStart_toStartOf="@+id/textView30"
                app:layout_constraintTop_toBottomOf="@+id/textView30" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</LinearLayout>